{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "ES2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "es2018", "dom"],
    "paths": {
      "lib": ["dist/angular-gauge-chart/lib"],
      "lib/*": ["dist/angular-gauge-chart/lib/*"],
      "angular-gauge-chart": ["dist/angular-gauge-chart"],
      "angular-gauge-chart/*": ["dist/angular-gauge-chart/*"]
    }
  }
}
